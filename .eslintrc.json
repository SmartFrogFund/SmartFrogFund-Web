{
    "extends": [
        "next/core-web-vitals",
        "airbnb"
    ],
    "rules": {
        "linebreak-style": [
            0,
            "unix"
        ],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx",
                    ".tsx"
                ]
            }
        ],
        "react/react-in-jsx-scope": 0,
        "import/extensions": 0,
        "no-console": 0,
        "import/prefer-default-export": 0,
        "max-len": 0,
        "no-unused-vars": 1,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": [
            0,
            {
                "commonjs": true,
                "amd": true
            }
        ]
    }
}